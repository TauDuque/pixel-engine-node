        "cors": "^2.8.5",
        "helmet": "^7.1.0",
        "dotenv": "^16.3.1",
        "swagger-ui-express": "^5.0.0",
        "swagger-jsdoc": "^6.2.8",
        "crypto": "^1.0.1",
