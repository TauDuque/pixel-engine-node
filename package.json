{
    "name": "pixel-engine-api",
    "version": "1.0.0",
    "description": "API REST para procesado de imÃ¡genes y consulta de tareas",
    "main": "dist/index.js",
    "scripts": {
        "build": "tsc",
        "start": "node dist/index.js",
        "dev": "ts-node-dev --respawn --transpile-only src/index.ts",
        "test": "jest",
        "test:watch": "jest --watch",
        "test:coverage": "jest --coverage",
        "lint": "eslint src/**/*.ts",
        "lint:fix": "eslint src/**/*.ts --fix",
        "db:init": "npm run build && node scripts/init-database.js",
        "db:clear": "npm run build && node -e \"require('./dist/config/database').Database.getInstance().clearDatabase().then(() => process.exit(0))\"",
        "setup": "node scripts/setup-env.js"
    },
    "keywords": [
        "api",
